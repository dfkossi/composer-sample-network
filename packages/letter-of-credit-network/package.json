{
  "engines": {
    "composer": "^0.20.0"
  },
  "name": "letter-of-credit-network",
  "version": "0.0.2",
  "description": "TPE Lettre de credit pour le commerce Network",
  "networkImage": "https://github.com/dfkossi/composer-sample-network/packages/letter-of-credit-network/networkimage.svg",
  "networkImageanimated": "https://github.com/dfkossi/composer-sample-network/packages/letter-of-credit-network/networkimageanimated.svg",
  "scripts": {

  },
  "keywords": [
    "letter",
    "credit",
    "composer",
    "composer-network"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dfkossi/composer-sample-network.git"
  },
  "author": "peniel winchester/ibm",
  "license": "ISC",
  "devDependencies": {
    "browserfs": "^1.2.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "composer-admin": "^0.20.0-0",
    "composer-cli": "^0.20.0-0",
    "composer-client": "^0.20.0-0",
    "composer-common": "^0.20.0-0",
    "composer-connector-embedded": "^0.20.0-0",
    "eslint": "^3.6.1",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.5.5",
    "license-check-and-add": "~2.3.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "moment": "^2.17.1",
    "nyc": "^11.0.2"
  },
  "license-check-and-add-config": {
    "folder": ".",
    "license": "LICENSE.txt",
    "exact_paths_method": "EXCLUDE",
    "exact_paths": [
      "composer-logs",
      "dist",
      "node_modules",
      "out",
      ".git"
    ],
    "file_type_method": "EXCLUDE",
    "file_types": [
      ".yml"
    ],
    "insert_license": false,
    "license_formats": {
      "js|cto|acl|qry": {
        "prepend": "/*",
        "append": " */",
        "eachLine": {
          "prepend": " * "
        }
      },
      "md": {
        "file": "../../markdown-license.txt"
      }
    }
  }
}
